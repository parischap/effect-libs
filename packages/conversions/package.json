{
  "name": "@parischap/conversions",
  "description": "A functional library to replace partially the native Intl API",
  "type": "module",
  "author": "Jérôme MARTIN",
  "license": "MIT",
  "devDependencies": {
    "@parischap/configs": "git+ssh://github.com/parischap/configs",
    "@effect/language-service": "0.64.1",
    "@types/node": "25.0.9",
    "@typescript/native-preview": "7.0.0-dev.20260206.1",
    "prettier-plugin-jsdoc": "1.8.0",
    "prettier-plugin-organize-imports": "4.3.0",
    "@tsconfig/strictest": "2.0.8",
    "prettier": "3.7.4",
    "oxlint": "1.43.0",
    "oxlint-tsgolint": "0.11.4",
    "unrun": "0.2.24",
    "vitest": "4.0.18",
    "madge": "8.0.0",
    "@effect/experimental": "0.58.0",
    "vite": "npm:rolldown-vite@7.3.0",
    "@babel/core": "7.28.5",
    "babel-plugin-annotate-pure-calls": "0.5.0",
    "@babel/plugin-transform-export-namespace-from": "7.27.1",
    "@babel/plugin-transform-modules-commonjs": "7.27.1",
    "@effect/cluster": "0.56.1",
    "@effect/platform": "0.94.2",
    "@effect/platform-node": "0.104.1",
    "@effect/rpc": "0.73.0",
    "@effect/sql": "0.49.0",
    "@effect/workflow": "0.16.0",
    "@effect/docgen": "0.5.2",
    "tsx": "4.21.0"
  },
  "scripts": {
    "test": "vitest run",
    "tscheck": "tsgo -b tsconfig.json --force",
    "lint": "oxlint --type-aware --type-check --report-unused-disable-directives --disable-nested-config",
    "lint-fix": "oxlint --type-aware --fix",
    "format": "prettier .",
    "clean": "unrun node_modules/@parischap/configs/esm/bin/clean.ts -activePackageOnly",
    "clean-config-files": "unrun node_modules/@parischap/configs/esm/bin/clean-config-files.ts -activePackageOnly",
    "clean-node-modules": "unrun node_modules/@parischap/configs/esm/bin/clean-node-modules.ts -activePackageOnly",
    "clean-prod": "pnpm rmrf dist && pnpm mkdirp dist",
    "generate-config-files": "unrun node_modules/@parischap/configs/esm/bin/generate-config-files.ts -activePackageOnly",
    "clean-all": "unrun node_modules/@parischap/configs/esm/bin/clean.ts",
    "clean-all-config-files": "unrun node_modules/@parischap/configs/esm/bin/clean-config-files.ts",
    "clean-all-node-modules": "unrun node_modules/@parischap/configs/esm/bin/clean-node-modules.ts",
    "clean-all-prod": "unrun node_modules/@parischap/configs/esm/bin/clean-prod.ts",
    "generate-all-config-files": "unrun node_modules/@parischap/configs/esm/bin/generate-config-files.ts",
    "update-all-exports": "unrun node_modules/@parischap/configs/esm/bin/update-exports.ts",
    "watch-all-for-exports": "unrun node_modules/@parischap/configs/esm/bin/update-exports.ts -watch",
    "reinstall-all-dependencies": "pnpm i --force",
    "build": "pnpm clean-prod && pnpm compile && cd dist && pnpm i && cd ..",
    "checks": "pnpm circular && pnpm lint && pnpm test",
    "circular": "madge --extensions ts --circular --no-color --no-spinner esm",
    "examples": "unrun examples/perf.ts&&unrun examples/RoundingOption_1.ts&&unrun examples/RoundingOption_2.ts&&unrun examples/RoundingOption_3.ts&&unrun examples/NumberBase10Format_1.ts&&unrun examples/NumberBase10Format_2.ts&&unrun examples/Template_1.ts&&unrun examples/Template_2.ts&&unrun examples/Template_3.ts&&unrun examples/DateTime_1.ts&&unrun examples/DateTimeFormat_1.ts&&unrun examples/DateTimeFormat_2.ts&&unrun examples/Branding.ts",
    "tsconfig": "tsc --showConfig --project tsconfig.esm.json",
    "update-exports": "unrun node_modules/@parischap/configs/esm/bin/update-exports.ts -activePackageOnly",
    "watch-for-exports": "unrun node_modules/@parischap/configs/esm/bin/update-exports.ts -watch -activePackageOnly",
    "build-and-publish": "pnpm build && pnpm checks && pnpm publish-to-npm",
    "publish-to-npm": "cd dist && npm publish --access=public && cd ..",
    "compile": "vite build",
    "docgen": "docgen"
  },
  "exports": {
    ".": {
      "default": "./esm/index.ts"
    },
    "./CVBigDecimal": {
      "default": "./esm/primitive/BigDecimal.ts"
    },
    "./CVBigInt": {
      "default": "./esm/primitive/BigInt.ts"
    },
    "./CVDateTime": {
      "default": "./esm/DateTime/index.ts"
    },
    "./CVDateTimeConstants": {
      "default": "./esm/DateTime/dateTimeConstants.ts"
    },
    "./CVDateTimeFormat": {
      "default": "./esm/formatting/DateTimeFormat.ts"
    },
    "./CVDateTimeFormatContext": {
      "default": "./esm/formatting/DateTimeFormatContext.ts"
    },
    "./CVEmail": {
      "default": "./esm/primitive/Email.ts"
    },
    "./CVGregorianDate": {
      "default": "./esm/internal/datetime/GregorianDate.ts"
    },
    "./CVInt": {
      "default": "./esm/internal/primitive/Int.ts"
    },
    "./CVInteger": {
      "default": "./esm/primitive/Integer.ts"
    },
    "./CVIsoDate": {
      "default": "./esm/internal/datetime/IsoDate.ts"
    },
    "./CVNumberBase10Format": {
      "default": "./esm/formatting/NumberBase10Format.ts"
    },
    "./CVPlaceholder": {
      "default": "./esm/formatting/TemplatePart/Placeholder/index.ts"
    },
    "./CVPlaceholderFormatter": {
      "default": "./esm/formatting/TemplatePart/Placeholder/PlaceholderFormatter.ts"
    },
    "./CVPlaceholderParser": {
      "default": "./esm/formatting/TemplatePart/Placeholder/PlaceholderParser.ts"
    },
    "./CVPositive": {
      "default": "./esm/internal/primitive/Positive.ts"
    },
    "./CVPositiveInteger": {
      "default": "./esm/primitive/PositiveInteger.ts"
    },
    "./CVPositiveReal": {
      "default": "./esm/primitive/PositiveReal.ts"
    },
    "./CVReal": {
      "default": "./esm/primitive/Real.ts"
    },
    "./CVRoundingMode": {
      "default": "./esm/formatting/RoundingMode.ts"
    },
    "./CVRoundingOption": {
      "default": "./esm/formatting/RoundingOption.ts"
    },
    "./CVSchema": {
      "default": "./esm/Schema.ts"
    },
    "./CVSemVer": {
      "default": "./esm/primitive/SemVer.ts"
    },
    "./CVSeparator": {
      "default": "./esm/formatting/TemplatePart/Separator.ts"
    },
    "./CVTemplate": {
      "default": "./esm/formatting/Template.ts"
    },
    "./CVTemplatePart": {
      "default": "./esm/formatting/TemplatePart/index.ts"
    },
    "./CVTemplateParts": {
      "default": "./esm/internal/formatting/TemplateParts.ts"
    },
    "./CVTime": {
      "default": "./esm/internal/datetime/Time.ts"
    },
    "./CVZoneOffsetParts": {
      "default": "./esm/internal/datetime/ZoneOffsetParts.ts"
    }
  },
  "module": "./esm/index.ts",
  "peerDependencies": {
    "@parischap/effect-lib": "^0.11.0",
    "effect": "3.19.15"
  }
}
