{
  "description": "A functional library to replace partially the native Intl API",
  "peerDependencies": {
    "@parischap/effect-lib": "^0.11.0"
  },
  "examples": [
    "perf.ts",
    "RoundingOption_1.ts",
    "RoundingOption_2.ts",
    "RoundingOption_3.ts",
    "NumberBase10Format_1.ts",
    "NumberBase10Format_2.ts",
    "Template_1.ts",
    "Template_2.ts",
    "Template_3.ts",
    "DateTime_1.ts",
    "DateTimeFormat_1.ts",
    "DateTimeFormat_2.ts",
    "Branding.ts"
