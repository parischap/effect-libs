{
  "name": "@parischap/ansi-styles",
  "description": "A functional library to style your strings in terminals",
  "type": "module",
  "author": "Jérôme MARTIN",
  "license": "MIT",
  "devDependencies": {
    "@parischap/configs": "git+ssh://github.com/parischap/configs",
    "@effect/language-service": "0.64.1",
    "@types/node": "25.0.9",
    "@typescript/native-preview": "7.0.0-dev.20260217.1",
    "prettier-plugin-jsdoc": "1.8.0",
    "prettier-plugin-organize-imports": "4.3.0",
    "@tsconfig/strictest": "2.0.8",
    "prettier": "3.7.4",
    "oxlint": "1.48.0",
    "oxlint-tsgolint": "0.14.0",
    "unrun": "0.2.27",
    "vitest": "4.0.18",
    "madge": "8.0.0",
    "@effect/experimental": "0.58.0",
    "vite": "npm:rolldown-vite@7.3.1",
    "@effect/cluster": "0.56.1",
    "@effect/platform": "0.94.2",
    "@effect/platform-node": "0.104.1",
    "@effect/rpc": "0.73.0",
    "@effect/sql": "0.49.0",
    "@effect/workflow": "0.16.0",
    "@effect/docgen": "0.5.2",
    "tsx": "4.21.0"
  },
  "scripts": {
    "test": "vitest run",
    "tscheck": "tsgo -b tsconfig.json --force",
    "lint": "oxlint --type-aware --type-check --report-unused-disable-directives --disable-nested-config",
    "lint-fix": "oxlint --type-aware --fix",
    "format": "prettier .",
    "clean": "unrun node_modules/@parischap/configs/esm/bin/clean.ts -activePackageOnly",
    "clean-config-files": "unrun node_modules/@parischap/configs/esm/bin/clean-config-files.ts -activePackageOnly",
    "generate-config-files": "unrun node_modules/@parischap/configs/esm/bin/generate-config-files.ts -activePackageOnly",
    "clean-all": "unrun node_modules/@parischap/configs/esm/bin/clean.ts",
    "clean-all-config-files": "unrun node_modules/@parischap/configs/esm/bin/clean-config-files.ts",
    "clean-all-node-modules": "unrun node_modules/@parischap/configs/esm/bin/clean-node-modules.ts",
    "clean-all-prod": "unrun node_modules/@parischap/configs/esm/bin/clean-prod.ts",
    "generate-all-config-files": "unrun node_modules/@parischap/configs/esm/bin/generate-config-files.ts",
    "update-all-exports": "unrun node_modules/@parischap/configs/esm/bin/update-exports.ts",
    "watch-all-for-exports": "unrun node_modules/@parischap/configs/esm/bin/update-exports.ts -watch",
    "reinstall-all-dependencies": "pnpm i --force",
    "build": "pnpm clean-prod && pnpm compile && cd dist && pnpm i && cd ..",
    "checks": "pnpm circular && pnpm lint && pnpm test",
    "circular": "madge --extensions ts --circular --no-color --no-spinner esm",
    "clean-node-modules": "unrun node_modules/@parischap/configs/esm/bin/clean-node-modules.ts -activePackageOnly",
    "clean-prod": "unrun node_modules/@parischap/configs/esm/bin/clean-prod.ts -activePackageOnly",
    "examples": "unrun examples/basic-usage.ts&&unrun examples/cancelling-a-style.ts&&unrun examples/simple-colors.ts&&unrun examples/all-colors.ts",
    "tsconfig": "tsc --showConfig --project tsconfig.esm.json",
    "update-exports": "unrun node_modules/@parischap/configs/esm/bin/update-exports.ts -activePackageOnly",
    "watch-for-exports": "unrun node_modules/@parischap/configs/esm/bin/update-exports.ts -watch -activePackageOnly",
    "build-and-publish": "pnpm build && pnpm checks && pnpm publish-to-npm",
    "publish-to-npm": "cd dist && npm publish --access=public && cd ..",
    "compile": "vite build",
    "docgen": "docgen"
  },
  "exports": {
    ".": {
      "default": "./esm/index.ts"
    },
    "./ASBackgroundColorStyleCharacteristic": {
      "default": "./esm/internal/style-characteristic/OptionalStyleCharacteristic/ColorOptionalStyleCharacteristic/BackgroundColorStyleCharacteristic.ts"
    },
    "./ASBlinkingStyleCharacteristic": {
      "default": "./esm/internal/style-characteristic/OptionalStyleCharacteristic/OnOffOptionalStyleCharacteristic/BlinkingStyleCharacteristic.ts"
    },
    "./ASBoldStyleCharacteristic": {
      "default": "./esm/internal/style-characteristic/OptionalStyleCharacteristic/OnOffOptionalStyleCharacteristic/BoldStyleCharacteristic.ts"
    },
    "./ASCode": {
      "default": "./esm/internal/Code.ts"
    },
    "./ASColor": {
      "default": "./esm/Color/index.ts"
    },
    "./ASColorOptionalStyleCharacteristic": {
      "default": "./esm/internal/style-characteristic/OptionalStyleCharacteristic/ColorOptionalStyleCharacteristic/index.ts"
    },
    "./ASConstantContextStyler": {
      "default": "./esm/ContextStyler/ConstantContextStyler.ts"
    },
    "./ASContextStyler": {
      "default": "./esm/ContextStyler/index.ts"
    },
    "./ASDimStyleCharacteristic": {
      "default": "./esm/internal/style-characteristic/OptionalStyleCharacteristic/OnOffOptionalStyleCharacteristic/DimStyleCharacteristic.ts"
    },
    "./ASEightBitColor": {
      "default": "./esm/Color/EightBitColor.ts"
    },
    "./ASEightBitColorCode": {
      "default": "./esm/internal/Color/EightBitColorCode.ts"
    },
    "./ASForegroundColorStyleCharacteristic": {
      "default": "./esm/internal/style-characteristic/OptionalStyleCharacteristic/ColorOptionalStyleCharacteristic/ForegroundColorStyleCharacteristic.ts"
    },
    "./ASHiddenStyleCharacteristic": {
      "default": "./esm/internal/style-characteristic/OptionalStyleCharacteristic/OnOffOptionalStyleCharacteristic/HiddenStyleCharacteristic.ts"
    },
    "./ASInversedStyleCharacteristic": {
      "default": "./esm/internal/style-characteristic/OptionalStyleCharacteristic/OnOffOptionalStyleCharacteristic/InversedStyleCharacteristic.ts"
    },
    "./ASItalicStyleCharacteristic": {
      "default": "./esm/internal/style-characteristic/OptionalStyleCharacteristic/OnOffOptionalStyleCharacteristic/ItalicStyleCharacteristic.ts"
    },
    "./ASOnOffOptionalStyleCharacteristic": {
      "default": "./esm/internal/style-characteristic/OptionalStyleCharacteristic/OnOffOptionalStyleCharacteristic/index.ts"
    },
    "./ASOptionalStyleCharacteristic": {
      "default": "./esm/internal/style-characteristic/OptionalStyleCharacteristic/index.ts"
    },
    "./ASOverlinedStyleCharacteristic": {
      "default": "./esm/internal/style-characteristic/OptionalStyleCharacteristic/OnOffOptionalStyleCharacteristic/OverlinedStyleCharacteristic.ts"
    },
    "./ASPalette": {
      "default": "./esm/Palette.ts"
    },
    "./ASPaletteContextStyler": {
      "default": "./esm/ContextStyler/PaletteContextStyler.ts"
    },
    "./ASRgbColor": {
      "default": "./esm/Color/RgbColor.ts"
    },
    "./ASSequence": {
      "default": "./esm/internal/Sequence.ts"
    },
    "./ASStruckThroughStyleCharacteristic": {
      "default": "./esm/internal/style-characteristic/OptionalStyleCharacteristic/OnOffOptionalStyleCharacteristic/StruckThroughStyleCharacteristic.ts"
    },
    "./ASStyle": {
      "default": "./esm/Style.ts"
    },
    "./ASStyleCharacteristics": {
      "default": "./esm/internal/StyleCharacteristics.ts"
    },
    "./ASStyles": {
      "default": "./esm/internal/Styles.ts"
    },
    "./ASText": {
      "default": "./esm/Text.ts"
    },
    "./ASThreeBitColor": {
      "default": "./esm/Color/ThreeBitColor.ts"
    },
    "./ASThreeBitColorOffset": {
      "default": "./esm/internal/Color/ThreeBitColorOffset.ts"
    },
    "./ASUnderlinedStyleCharacteristic": {
      "default": "./esm/internal/style-characteristic/OptionalStyleCharacteristic/OnOffOptionalStyleCharacteristic/UnderlinedStyleCharacteristic.ts"
    },
    "./ASUnistyledText": {
      "default": "./esm/internal/UnistyledText.ts"
    }
  },
  "module": "./esm/index.ts",
  "peerDependencies": {
    "@parischap/effect-lib": "^0.11.0",
    "effect": "3.19.15"
  }
}